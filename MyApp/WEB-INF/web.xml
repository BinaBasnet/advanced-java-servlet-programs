    <servlet-class>SimpleInterestServlet</servlet-class>
</servlet>
<servlet-mapping>
    <servlet-name>SimpleInterestServlet</servlet-name>
    <url-pattern>/SimpleInterestServlet</url-pattern>
</servlet-mapping>




    <servlet>
